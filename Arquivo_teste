using System;


namespace Testes_Gerais
{
    class Car
    {
        string num = "10";
        int price = int.Parse(num); // O problema está aqui quando esse arquivo faz parte de uma solução. O VS pede que NUM seja STATIC ou que se crie um objeto.

        public void Comprar()
        {
            price += 200;
            Console.WriteLine($"The car's price is {price}.");
        }
    }
}
